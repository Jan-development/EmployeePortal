  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "@types/react-i18next": "^7.8.3",
    "dompurify": "^3.2.4",
    "i18next": "^24.2.2",
    "msw": "^1.3.3",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
